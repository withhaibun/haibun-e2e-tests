{
  "type": "module",
  "publish": "true",
  "name": "@haibun/e2e-tests",
  "version": "1.56.0",
  "description": "e2e tests for haibun",
  "files": [
    "build",
    "local",
    "publish",
    "files"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build-watch": "tsc --watch",
    "build": "tsc",
    "test": "HAIBUN_O_WEBPLAYWRIGHT_MONITOR=each HAIBUN_O_OUTXUNIT_STORAGE=StorageFS HAIBUN_O_A11YSTEPPER_STORAGE=StorageFS HAIBUN_KEY=test HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli local",
    "run-walkthrough": "HAIBUN_O_A11YSTEPPER_STORAGE=StorageFS HAIBUN_KEY=test HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS haibun-cli walkthrough",
    "test-vars": "HAIBUN_O_WEBPLAYWRIGHT_MONITOR=each HAIBUN_O_OUTXUNIT_STORAGE=StorageFS HAIBUN_O_A11YSTEPPER_STORAGE=StorageFS HAIBUN_KEY=test HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli vars",
    "walkthrough": "run-vcapture --tts run-walkthrough walkthrough build-local files",
    "test-chromium": "HAIBUN_SPLIT_SHARED=a-browser=chromium npm run test",
    "test-some": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=5 haibun-cli local",
    "test-many": "HAIBUN_LOOPS=5 HAIBUN_MEMBERS=50 haibun-cli local",
    "test-firefox": "HAIBUN_SPLIT_SHARED=a-browser=firefox haibun-cli local",
    "test-webkit": "HAIBUN_SPLIT_SHARED=a-browser=webkit haibun-cli local",
    "test-local": "HAIBUN_SPLIT_SHARED='a-browser=chromium,webkit,firefox,chromium.iPhone 6,chromium.iPad (gen 7) landscape,chromium.iPad Mini,chromium.Pixel 5,chromium.iPhone X landscape,chromium.Kindle Fire HDX,chromium.Moto G4 landscape' haibun-cli local",
    "test-envc": "HAIBUN_ENVC=what=what1,what=what2,also=also1,also=also2 haibun-cli envc",
    "test-envc-10": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=5 HAIBUN_ENVC=what=what1,what=what2,also=also1,also=also2 haibun-cli envc",
    "publish": "HAIBUN_O_OUTREVIEWS_PUBLISH_ROOT=./reviews HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_PUBLISH_STORAGE=StorageFS haibun-cli --config publish/local publish"
  },
  "keywords": [],
  "author": "vid",
  "license": "ISC",
  "dependencies": {
    "@haibun/cli": "^1.56",
    "@haibun/core": "^1.56",
    "@haibun/domain-storage": "^1.56",
    "@haibun/out-xunit": "^1.56",
    "@haibun/storage-fs": "^1.56",
    "@haibun/utils": "^1.56",
    "@haibun/web-accessibility-axe": "~1.49",
    "@haibun/web-http": "^1.56",
    "@haibun/web-playwright": "^1.56",
    "@haibun/web-server-express": "^1.56",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^29.5.11",
    "express-fileupload": "^1.4.0",
    "typescript": "~5.8"
  }
}
